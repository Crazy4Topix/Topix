{
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "singleQuote": true,
    "semi": true,
    "printWidth": 100,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "tailwindConfig": "./tailwind.config.js",
    "tailwindAttributes": ["className"],
    "plugins": [
        "prettier-plugin-tailwindcss"
    ]
}
