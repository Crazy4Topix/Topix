{
  "expo": {
    "name": "Topix",
    "slug": "topix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TopixIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/TopixIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TopixForeground.png",
        "backgroundColor": "#000000",
        "monochromeImage": "./assets/images/TopixForeground.png"
      },
      "package": "com.topix.topix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe5892ab-28fd-47b0-b604-ce39046adea3"
      }
    },
    "owner": "crazy-four-topix"
  }
}
