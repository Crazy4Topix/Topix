{
  "expo": {
    "name": "Topix",
    "slug": "topix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TopixLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/TopixLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TopixLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.topix.topix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e6a8a4e1-af24-45fd-a5a5-35731c31a606"
      }
    }
  }
}
